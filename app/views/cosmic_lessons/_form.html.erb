<%= simple_form_for(@cosmic_lesson) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :seo_key %>
    <%= f.input :cosmic_domain %>
    <%= f.input :cosmic_domain_order %>
    <%= f.input :title_short %>
    <%= f.input :title_long %>
    <%= f.input :overview %>
    <%= f.input :content %>
    <%= f.input :type %>
    <%= f.input :custom1 %>
    <%= f.input :custom2 %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
